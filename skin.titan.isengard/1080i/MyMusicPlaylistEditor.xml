<?xml version="1.0" encoding="utf-8"?>
<!--Titan-->
<window id="503">
    <defaultcontrol allways="true">50</defaultcontrol>
    <allowoverlay>yes</allowoverlay>
	
	<onunload>ClearProperty(ShowViewSubMenu, Home)</onunload>
    <views>50</views>
	<include>animation_window_open_close</include>
    <controls>
        <!--Music Background-->
        <include>GlobalBackground</include>
        <!--Time & Weather-->
        <include>TimeInfo</include>
        <!--Header-->
        <include>Header</include>
        <!-- footer -->
		<control type="image">	
			<posx>50</posx>
			<posy>960</posy>
			<width>1820</width>
			<height>84</height>
			<texture>home/home_rss.png</texture>
			<visible>[!Skin.HasSetting(NoFooterBar) + skin.hastheme(classic)]</visible>	
			<include>animation_fade_visible_hidden</include>				
		</control>
		<control type="image">
			<posx>0</posx>
			<posy>982</posy>
			<width>1920</width>
			<height>110</height>
			<texture>colors/color_white.png</texture>
			<colordiffuse>$VAR[panel_color]</colordiffuse>
			<visible>!skin.hastheme(classic) + !skin.hassetting(nofooterbar)</visible>
			<include>animation_fade_visible_hidden</include>
			<animation effect="fade" end="80" time="0" reversible="false" condition="StringCompare(Skin.String(FooterOpacity),80)">Conditional</animation>
			<animation effect="fade" end="60" time="0" reversible="false" condition="StringCompare(Skin.String(FooterOpacity),60)">Conditional</animation>
			<animation effect="fade" end="40" time="0" reversible="false" condition="StringCompare(Skin.String(FooterOpacity),40)">Conditional</animation>
			<animation effect="fade" end="20" time="0" reversible="false" condition="StringCompare(Skin.String(FooterOpacity),20)">Conditional</animation>
			<animation effect="fade" end="0" time="0" reversible="false" condition="StringCompare(Skin.String(FooterOpacity),0)">Conditional</animation>
		</control>
        
		
		
		<!--Panel-->
        <control type="image">
            <posx>60</posx>
            <posy>240</posy>
            <width>1800</width>
            <height>660</height>
            <texture>colors/color_white.png</texture>
			<animation effect="fade" end="90" time="0" condition="true">Conditional</animation>
			<colordiffuse>$VAR[panel_color]</colordiffuse>
        </control>
        <control type="group">
            <!--Start Left Panel-->
            <description>Left panel</description>
            <posx>20</posx>
            <posy>300</posy>
            <control type="image">
				<posx>80</posx>
				<posy>-20</posy>
				<width>850</width>
				<height>600</height>
				<texture>colors/color_white.png</texture>
				<animation effect="fade" end="90" time="0" condition="true">Conditional</animation>
				<colordiffuse>$VAR[panel_color]</colordiffuse>
			</control>
            
            <control type="label">
                <!--Header-->
                <posx>150</posx>
                <width>700</width>
                <align>left</align>
                <label>$LOCALIZE[1217]</label>
                <textcolor>white</textcolor>
                <font>Reg30</font>
            </control>
            <control type="list" id="50">
                <!--List 50-->
                <posx>0</posx>
                <posy>100</posy>
                <width>950</width>
                <height>420</height>
                <onleft>noop</onleft>
                <onright>100</onright>
                <onup>50</onup>
                <ondown>50</ondown>
                <itemlayout height="69" width="900">
                    <control type="image">
                        <posx>130</posx>
                        <posy>7</posy>
                        <width>64</width>
                        <height>53</height>
                        <texture>common/folder.png</texture>
                        <colordiffuse>FFffffff</colordiffuse>
                    </control>
                    <control type="label">
                        <posx>210</posx>
                        <width>480</width>
                        <align>left</align>
                        <font>Reg30</font>
                        <info>ListItem.Label</info>
                        <textcolor>white</textcolor>
                    </control>
                    <control type="label">
                        <posx>890</posx>
                        <width>200</width>
                        <align>right</align>
                        <font>Reg30</font>
                        <info>ListItem.Label2</info>
                        <textcolor>white</textcolor>
                    </control>
                </itemlayout>
                <focusedlayout height="69" width="900">
                    <control type="image">
                        <visible>Control.HasFocus(50)</visible>
                        <posx>100</posx>
                        <posy>7</posy>
                        <width>850</width>
                        <height>50</height>
                        <texture>common/slider.png</texture>
                        <colordiffuse>DDffffff</colordiffuse>
                    </control>
                    <control type="image">
                        <posx>130</posx>
                        <posy>7</posy>
                        <width>64</width>
                        <height>53</height>
                        <texture>common/folder.png</texture>
                        <colordiffuse>FFffffff</colordiffuse>
                    </control>
                    <control type="label">
                        <posx>210</posx>
                        <width>480</width>
                        <align>left</align>
                        <font>Reg30</font>
                        <info>ListItem.Label</info>
                        <scrollspeed>30</scrollspeed>
                        <textcolor>mainblue</textcolor>
                    </control>
                    <control type="label">
                        <posx>890</posx>
                        <width>200</width>
                        <align>right</align>
                        <font>Reg30</font>
                        <info>ListItem.Label2</info>
                        <scrollspeed>30</scrollspeed>
                        <textcolor>mainblue</textcolor>
                    </control>
                </focusedlayout>
            </control>
            <control type="label">
                <!--Object Info RSS-->
                <description>Header Label</description>
                <posx>150</posx>
                <posy>680</posy>
                <width>480</width>
                <align>left</align>
                <label>$INFO[Container(50).ListItem.Label]</label>
                <font>Reg30</font>
                <textcolor>white</textcolor>
            </control>
            <control type="label">
                <!--Page Info RSS-->
                <posx>870</posx>
                <posy>680</posy>
                <width>250</width>
                <align>right</align>
                <font>Reg30</font>
                <textcolor>white</textcolor>
                <label>$INFO[Container(50).CurrentPage]/$INFO[Container(50).NumPages]</label>
            </control>
        </control>
        <control type="group">
            <!--Start Right Panel-->
            <description>Right panel</description>
            <posx>900</posx>
            <posy>300</posy>
            <control type="image">
				<posx>80</posx>
				<posy>-20</posy>
				<width>850</width>
				<height>600</height>
				<texture>colors/color_white.png</texture>
				<animation effect="fade" end="90" time="0" condition="true">Conditional</animation>
				<colordiffuse>$VAR[panel_color]</colordiffuse>
			</control>
            <control type="label">
                <!--Header-->
                <posx>150</posx>
                <width>700</width>
                <align>left</align>
                <label>$LOCALIZE[525]</label>
                <textcolor>white</textcolor>
                <font>Reg30</font>
            </control>
            <control type="list" id="100">
                <!--List 100-->
                <posx>0</posx>
                <posy>100</posy>
                <width>950</width>
                <height>420</height>
                <onleft>50</onleft>
                <onright>noop</onright>
                <onup>100</onup>
                <ondown>100</ondown>
                <itemlayout height="69" width="900">
                    <control type="image">
                        <posx>130</posx>
                        <posy>7</posy>
                        <width>64</width>
                        <height>53</height>
                        <texture>common/folder.png</texture>
                        <colordiffuse>FFffffff</colordiffuse>
                    </control>
                    <control type="label">
                        <posx>210</posx>
                        <width>480</width>
                        <align>left</align>
                        <font>Reg30</font>
                        <info>ListItem.Label</info>
                        <textcolor>white</textcolor>
                    </control>
                    <control type="label">
                        <posx>890</posx>
                        <width>200</width>
                        <align>right</align>
                        <font>Reg30</font>
                        <info>ListItem.Label2</info>
                        <textcolor>white</textcolor>
                    </control>
                </itemlayout>
                <focusedlayout height="69" width="900">
                    <control type="image">
                        <visible>Control.HasFocus(100)</visible>
                        <posx>100</posx>
                        <posy>7</posy>
                        <width>850</width>
                        <height>50</height>
                        <texture>common/slider.png</texture>
                        <colordiffuse>DDffffff</colordiffuse>
                    </control>
                    <control type="image">
                        <posx>130</posx>
                        <posy>7</posy>
                        <width>64</width>
                        <height>53</height>
                        <texture>common/folder.png</texture>
                        <colordiffuse>FFffffff</colordiffuse>
                    </control>
                    <control type="label">
                        <posx>210</posx>
                        <width>480</width>
                        <align>left</align>
                        <font>Reg30</font>
                        <info>ListItem.Label</info>
                        <scrollspeed>30</scrollspeed>
                        <textcolor>mainblue</textcolor>
                    </control>
                    <control type="label">
                        <posx>890</posx>
                        <width>200</width>
                        <align>right</align>
                        <font>Reg30</font>
                        <info>ListItem.Label2</info>
                        <scrollspeed>30</scrollspeed>
                        <textcolor>mainblue</textcolor>
                    </control>
                </focusedlayout>
            </control>
            <control type="label">
                <!--Object Info RSS-->
                <description>Header Label</description>
                <posx>150</posx>
                <posy>680</posy>
                <width>480</width>
                <align>left</align>
                <label>$INFO[Container(100).ListItem.Label]</label>
                <font>Reg30</font>
                <textcolor>white</textcolor>
            </control>
            <control type="label">
                <!--Page Info RSS-->
                <posx>870</posx>
                <posy>680</posy>
                <width>250</width>
                <align>right</align>
                <font>Reg30</font>
                <textcolor>white</textcolor>
                <label>$INFO[Container(100).CurrentPage]/$INFO[Container(100).NumPages]</label>
            </control>
        </control>
    </controls>
</window>