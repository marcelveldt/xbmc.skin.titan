<?xml version="1.0" encoding="utf-8"?>
<!--Titan-->
<window id="133">
    <defaultcontrol always="true">401</defaultcontrol>
    <allowoverlay>yes</allowoverlay>
    <controls>
        <control type="group">
            <posx>60</posx>
            <posy>20</posy>
            <animation type="WindowOpen" reversible="false">
				<effect type="slide" start="0,-400" end="0,0" tween="cubic" delay="500" easing="out" time="750" />
			</animation>
			<animation type="WindowClose" reversible="false">
				<effect type="slide" start="0,0" end="0,-400" tween="cubic" easing="out" time="750" />
			</animation>
            <visible>!Window.IsActive(DialogKayToast.xml) + !Control.IsVisible(77778)</visible>
            <control type="image">
				<posx>-120</posx>
				<posy>-40</posy>
				<width>2040</width>
				<height>180</height>
				<texture background="true">views/filemanager_panel1.png</texture>
				<visible>!skin.hastheme(classic)</visible>
			</control>
			<control type="group">
				<posx>-60</posx>
				<posy>-20</posy>
				<visible>!skin.hastheme(classic)</visible>
				<include>TimeInfoAlt</include>
			</control>
			<control type="multiimage">
                <!--Busy Image-->
                <description>Busy animation</description>
                <posx>0</posx>
                <posy>15</posy>
                <width>64</width>
                <height>64</height>
                <imagepath>dialogs/extended_progress_bar</imagepath>
                <timeperimage>100</timeperimage>
                <fadetime>0</fadetime>
            </control>
            <control type="label" id="30">
                <!--Header-->
                <posx>90</posx>
                <posy>0</posy>
                <width>900</width>
                <height>50</height>
                <font>Bold38</font>
                <textcolor>ccmainblue</textcolor>
                <label>-</label>
                <scroll>false</scroll>
            </control>
            <control type="label" id="31">
                <!--Current Directory Label-->
                <posx>90</posx>
                <posy>40</posy>
                <width>900</width>
                <height>50</height>
                <font>Reg30</font>
                <textcolor>eewhite</textcolor>
                <label>-</label>
                <scroll>false</scroll>
            </control>
        </control>
	
	</controls>
</window>