<?xml version="1.0" encoding="utf-8"?>
<!--Titan-->
<window>
    <defaultcontrol always="true">6</defaultcontrol>
    <controls>
        <control type="group">
            <!--Dialog Overlay-->
            <include condition="!skin.hastheme(classic)">GlobalBackground</include>
			<include condition="!skin.hastheme(classic)">DialogOverlay</include>
			<include condition="skin.hastheme(classic)">DialogOverlayExtra</include>
            <!--Dialog Picture Info-->
            <control type="group">
                <include>animation_dialog</include>
                <control type="button" id="6">
                    <onfocus>SetFocus(5)</onfocus>
                    <texturefocus>-</texturefocus>
                    <texturenofocus>-</texturenofocus>
                </control>
                
                <!--Panel-->
                <control type="image">
                    <posx>638</posx>
                    <posy>240</posy>
                    <width>644</width>
                    <height>541</height>
                    <texture border="5">dialogs/default/inner.png</texture>
                </control>
				<control type="label">
                    <description>Heading</description>
                    <posx>473</posx>
                    <posy>230</posy>
                    <width>974</width>
                    <label>$LOCALIZE[13406]</label>
                    <include>DialogHeader</include>
                    <align>center</align>
                </control>
                <control type="list" id="5">
                    <posx>638</posx>
                    <posy>300</posy>
                    <width>644</width>
                    <height>480</height>
                    <onleft>5</onleft>
                    <onright>5</onright>
                    <onup>5</onup>
                    <ondown>5</ondown>
                    <pagecontrol></pagecontrol>
                    <scrolltime tween="cubic" easing="out">500</scrolltime>
                    <itemlayout height="60">
                        <control type="image">
                            <posx>0</posx>
                            <posy>1</posy>
                            <width>644</width>
                            <height>60</height>
                            <texture>dialogs/default/separator.png</texture>
                        </control>
                        <control type="label">
                            <posx>20</posx>
                            <posy>0</posy>
                            <width>604</width>
                            <height>60</height>
                            <info>ListItem.Label</info>
                            <include>DialogListNF</include>
                            <align>left</align>
                            <aligny>center</aligny>
                            <scroll>false</scroll>
                        </control>
                        <control type="label">
                            <posx>624</posx>
                            <posy>0</posy>
                            <width>604</width>
                            <height>60</height>
                            <info>ListItem.Label2</info>
                            <include>DialogListNF</include>
                            <align>right</align>
                            <aligny>center</aligny>
                            <scroll>false</scroll>
                        </control>
                    </itemlayout>
                    <focusedlayout height="60">
                        <control type="group">
                            <visible>!Control.HasFocus(5)</visible>
                            <animation effect="fade" start="0" end="100" time="300" tween="sine" easing="inout" condition="![Container(5).Scrolling | Container(5).OnScrollNext | Container(5).OnScrollPrevious]">Visible</animation>
                            <animation effect="fade" start="100" end="0" time="300" tween="sine" easing="inout" condition="![Container(5).Scrolling | Container(5).OnScrollNext | Container(5).OnScrollPrevious]">Hidden</animation>
                            <control type="image">
                                <posx>0</posx>
                                <posy>1</posy>
                                <width>644</width>
                                <height>60</height>
                                <texture>dialogs/default/separator.png</texture>
                            </control>
                            <control type="label">
                                <posx>20</posx>
                                <posy>0</posy>
                                <width>604</width>
                                <height>60</height>
                                <info>ListItem.Label</info>
                                <include>DialogListNF</include>
                                <align>left</align>
                                <aligny>center</aligny>
                                <scroll>false</scroll>
                            </control>
                            <control type="label">
                                <posx>624</posx>
                                <posy>0</posy>
                                <width>604</width>
                                <height>60</height>
                                <info>ListItem.Label2</info>
                                <include>DialogListNF</include>
                                <align>right</align>
                                <aligny>center</aligny>
                                <scroll>false</scroll>
                            </control>
                        </control>
                        <control type="group">
                            <visible>Control.HasFocus(5)</visible>
                            <animation effect="fade" start="0" end="100" time="300" tween="sine" easing="inout" condition="![Container(5).Scrolling | Container(5).OnScrollNext | Container(5).OnScrollPrevious]">Visible</animation>
                            <animation effect="fade" start="100" end="0" time="300" tween="sine" easing="inout" condition="![Container(5).Scrolling | Container(5).OnScrollNext | Container(5).OnScrollPrevious]">Hidden</animation>
                            <control type="image">
                                <width>644</width>
                                <height>60</height>
                                <texture border="2">dialogs/default/highlight.png</texture>
                            </control>
                            <control type="label">
                                <posx>20</posx>
                                <posy>0</posy>
                                <width>604</width>
                                <height>60</height>
                                <info>ListItem.Label</info>
                                <include>DialogListFO</include>
                                <align>left</align>
                                <aligny>center</aligny>
                                <scroll>false</scroll>
                            </control>
                            <control type="label">
                                <posx>624</posx>
                                <posy>0</posy>
                                <width>604</width>
                                <height>60</height>
                                <info>ListItem.Label2</info>
                                <include>DialogListFO</include>
                                <align>right</align>
                                <aligny>center</aligny>
                            </control>
                        </control>
                    </focusedlayout>
                </control>
            </control>
        </control>
    </controls>
</window>