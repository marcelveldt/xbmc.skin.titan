<?xml version="1.0" encoding="utf-8"?>
<!--Titan-->
<window id="6">
    <defaultcontrol always="true">50</defaultcontrol>
    <allowoverlay>yes</allowoverlay>
    <views>50,51,52,501,505,53,54,55,506,56,57,58,59</views>
    <onload>Skin.Reset(youtube)</onload>
    <controls>
        <!-- Video Background -->
        <include>VideoBackground</include>
		<!--Time & Weather-->
        <include>TimeInfo</include>
		<!--NowPlaying-->
        <include condition="Skin.HasTheme(classic)">NowPlayingMusicClassic</include>
        <include condition="Skin.HasTheme(classic)">NowPlayingVideoClassic</include>
		<include condition="!Skin.HasTheme(classic)">NowPlayingModern</include>
        
        <!--Header-->
        <include>Header</include>
        <!--Hidden Button-->
        <control type="radiobutton" id="19">
            <description>Filter (hidden)</description>
            <include>HiddenButton</include>
        </control>
        <!--Views-->
        <include>View_50_List</include>
        <include>View_55_BannerList</include>
        <include>View_51_Showcase</include>
        <include>View_54_ShowcaseDetails</include>
        <include>View_52_Panel</include>
        <include>View_53_PanelDetails</include>
        <include>BannerView</include>
        <include>BannerViewDetails</include>
		<include>ViewExtendedList</include>
		<include>View_56_BannerPlex</include>
		<include>View_57_BigPanel</include>
		<include>View_58_LargePoster</include>
		<include>View_59_BigPanelDetails</include>
        <!--Sub Menu Tab Left-->
        <include>SubMenuTabLeft</include>
        <!--Sub Menu Tab Top-->
        <include>SubMenuTabTop</include>
        <!--Sub Menu-->
        <control type="group">
            <include condition="skin.hastheme(classic)">SideBladeClassic</include>
			<include condition="!skin.hastheme(classic)">SideBladeModern</include>
            <!--Gouplist 9000-->
            <control type="grouplist" id="9000">
                <posx>21</posx>
                <posy>30</posy>
                <width>458</width>
                <height>730</height>
                <itemgap>3</itemgap>
                <onleft>50</onleft>
                <onright>50</onright>
                <onup>9000</onup>
                <ondown>9000</ondown>
                <orientation>vertical</orientation>
                <!--Buttons-->
                <control type="label" id="200">
                    <!--Label Menu-->
                    <width>458</width>
                    <align>left</align>
                    <textoffsetx>50</textoffsetx>
                    <font>Bold40</font>
                    <textcolor>$VAR[ThemeFontColorBlack]</textcolor>
                    <label>$LOCALIZE[33061]:</label>
                </control>
                <control type="image" id="201">
                    <!--Seperator-->
                    <width>458</width>
                    <height>20</height>
                    <align>left</align>
                    <texture>common/menu-separator.png</texture>
                </control>
                <control type="radiobutton" id="14">
                    <!--Button Watched Toggle-->
                    <include>SubMenuButton</include>
                    <label>20367</label>
                </control>
                <control type="button" id="3">
                    <!--Button Sort-->
                    <include>SubMenuButton</include>
                    <align>left</align>
                </control>
				<control type="togglebutton" id="20">
					<description>Update library</description>
					<textwidth>235</textwidth>
					<include>SubMenuButton</include>
					<label>653</label>
					<altlabel>13353</altlabel>
					<alttexturefocus border="5">common/button-fo.png</alttexturefocus>
					<alttexturenofocus>-</alttexturenofocus>
					<usealttexture>library.isscanningvideo</usealttexture>
					<visible>!substring(Container.FolderPath,plugin://plugin.video)</visible>
				</control>
                <control type="button" id="8">
                    <!--Button Search-->
                    <include>SubMenuButton</include>
                    <label>137</label>
                </control>

				 <control type="button" id="400">
                    <!--Set View-->
                    <include>SubMenuButton</include>
                    <label>View: $INFO[Container.Viewmode]</label>
                    <onclick>Container.NextViewMode</onclick>
					<onclick condition="[substring(Container.FolderPath,plugin://plugin.video.xbmb3c) | Container.FolderPath,plugin://plugin.video.netflixbmc)] + Container.Content(Movies)]">XBMC.RunScript(script.titanskin.helpers,SETVIEW,MOVIES,00)</onclick>
                    <onclick condition="[substring(Container.FolderPath,plugin://plugin.video.xbmb3c) | Container.FolderPath,plugin://plugin.video.netflixbmc)] + Container.Content(seasons)]">XBMC.RunScript(script.titanskin.helpers,SETVIEW,SEASONS,00)</onclick>
                    <onclick condition="[substring(Container.FolderPath,plugin://plugin.video.xbmb3c) | Container.FolderPath,plugin://plugin.video.netflixbmc)] + Container.Content(TVShows)]">XBMC.RunScript(script.titanskin.helpers,SETVIEW,SERIES,00)</onclick>
                    <onclick condition="[substring(Container.FolderPath,plugin://plugin.video.xbmb3c) | Container.FolderPath,plugin://plugin.video.netflixbmc)] + Container.Content(Episodes)]">XBMC.RunScript(script.titanskin.helpers,SETVIEW,EPISODES,00)</onclick>
                    <onclick condition="[substring(Container.FolderPath,plugin://plugin.video.xbmb3c) | Container.FolderPath,plugin://plugin.video.netflixbmc)] + Container.Content(Sets)]">XBMC.RunScript(script.titanskin.helpers,SETVIEW,BOXSETS,00)</onclick>
				</control>
				
				<control type="radiobutton" id="341">
                    <!--Toggle Overlay-->
                    <include>SubMenuButton</include>
                    <label>31094</label>
                    <onclick>Skin.ToggleSetting(HideWatchedOverlayBannerPlex)</onclick>
                    <selected>Skin.HasSetting(HideWatchedOverlayBannerPlex)</selected>
					 <visible>Control.IsVisible(56)</visible>
                </control>

				<control type="radiobutton" id="307">
                    <!--Toggle Overlay-->
                    <include>SubMenuButton</include>
                    <label>31094</label>
                    <onclick>Skin.ToggleSetting(HideWatchedOverlayExtended)</onclick>
                    <selected>Skin.HasSetting(HideWatchedOverlayExtended)</selected>
					 <visible>Control.IsVisible(506)</visible>
                </control>
				<control type="radiobutton" id="308">
                    <!--Toggle Overlay-->
                    <include>SubMenuButton</include>
                    <label>31094</label>
                    <onclick>Skin.ToggleSetting(HideWatchedOverlayShowCase)</onclick>
                    <selected>Skin.HasSetting(HideWatchedOverlayShowCase)</selected>
					 <visible>Control.IsVisible(51) | Control.IsVisible(54)</visible>
                </control>
				<control type="radiobutton" id="309">
                    <!--Toggle Overlay-->
                    <include>SubMenuButton</include>
                    <label>31094</label>
                    <onclick>Skin.ToggleSetting(HideWatchedOverlayPoster)</onclick>
                    <selected>Skin.HasSetting(HideWatchedOverlayPoster)</selected>
					 <visible>Control.IsVisible(52) | Control.IsVisible(53)</visible>
                </control>
				<control type="radiobutton" id="310">
                    <!--Toggle Overlay-->
                    <include>SubMenuButton</include>
                    <label>31094</label>
                    <onclick>Skin.ToggleSetting(HideWatchedOverlayLandscape)</onclick>
                    <selected>Skin.HasSetting(HideWatchedOverlayLandscape)</selected>
					 <visible>Control.IsVisible(505) | Control.IsVisible(501)</visible>
                </control>
				<control type="radiobutton" id="311">
                    <!--Toggle Overlay-->
                    <include>SubMenuButton</include>
                    <label>31094</label>
                    <onclick>Skin.ToggleSetting(HideWatchedOverlayBanner)</onclick>
                    <selected>Skin.HasSetting(HideWatchedOverlayBanner)</selected>
					 <visible>Control.IsVisible(55)</visible>
                </control>
                <control type="radiobutton" id="312">
                    <!--Toggle Overlay-->
                    <visible>Container.Content(Movies) + [Control.IsVisible(505) | Control.IsVisible(501)]</visible>
                    <include>SubMenuButton</include>
                    <label>31063</label>
                    <onclick>Skin.ToggleSetting(MovieViewNoOverlay)</onclick>
                    <selected>Skin.HasSetting(MovieViewNoOverlay)</selected>
                </control>
                <control type="radiobutton" id="313">
                    <!--Toggle Overlay-->
                    <visible>Container.Content(TVShows) + [Control.IsVisible(505) | Control.IsVisible(501)]</visible>
                    <include>SubMenuButton</include>
                    <label>31063</label>
                    <onclick>Skin.ToggleSetting(TVshowsViewNoOverlay)</onclick>
                    <selected>Skin.HasSetting(TVshowsViewNoOverlay)</selected>
                </control>
                <control type="radiobutton" id="314">
                    <!--Toggle Overlay-->
                    <visible>Container.Content(seasons) + [Control.IsVisible(505) | Control.IsVisible(501)]</visible>
                    <include>SubMenuButton</include>
                    <label>31063</label>
                    <onclick>Skin.ToggleSetting(SeasonsViewNoOverlay)</onclick>
                    <selected>Skin.HasSetting(SeasonsViewNoOverlay)</selected>
                </control>
                <control type="radiobutton" id="315">
                    <!--Toggle Overlay-->
                    <visible>Container.Content(episodes) + [Control.IsVisible(505) | Control.IsVisible(501)]</visible>
                    <include>SubMenuButton</include>
                    <label>31063</label>
                    <onclick>Skin.ToggleSetting(EpisodesViewNoOverlay)</onclick>
                    <selected>Skin.HasSetting(EpisodesViewNoOverlay)</selected>
                </control>
                <control type="radiobutton" id="316">
                    <!--Toggle Overlay-->
                    <visible>Container.Content(sets) + [Control.IsVisible(505) | Control.IsVisible(501)]</visible>
                    <include>SubMenuButton</include>
                    <label>31063</label>
                    <onclick>Skin.ToggleSetting(BoxViewNoOverlay)</onclick>
                    <selected>Skin.HasSetting(BoxViewNoOverlay)</selected>
                </control>
				<control type="radiobutton" id="318">
                    <!--show fanart thumb in view 59-->
                    <visible>Control.IsVisible(59)</visible>
                    <include>SubMenuButton</include>
                    <label>31114</label>
                    <onclick>Skin.ToggleSetting(59_SmallFanart)</onclick>
                    <selected>Skin.HasSetting(59_SmallFanart)</selected>
                </control>
				<control type="radiobutton" id="319">
                    <!--Toggle Overlay-->
                    <include>SubMenuButton</include>
                    <label>31094</label>
                    <onclick>Skin.ToggleSetting(HideWatchedOverlayLargePoster)</onclick>
                    <selected>Skin.HasSetting(HideWatchedOverlayLargePoster)</selected>
					<visible>Control.IsVisible(58)</visible>
                </control>
				<control type="radiobutton" id="320">
                    <!--Show Fanart Background-->
                    <include>SubMenuButton</include>
                    <label>31141</label>
                    <onclick>Skin.ToggleSetting(ShowFanartBackground)</onclick>
                    <selected>Skin.HasSetting(ShowFanartBackground)</selected>
                </control>
				<control type="radiobutton" id="321">
                    <!--Hide panel Background-->
                    <include>SubMenuButton</include>
                    <label>31155</label>
                    <onclick>Skin.ToggleSetting(View55_HidePanel)</onclick>
                    <selected>Skin.HasSetting(View55_HidePanel)</selected>
					<visible>Control.IsVisible(55)</visible>
                </control>
				<control type="radiobutton" id="322">
                    <!--Hide panel Background-->
                    <include>SubMenuButton</include>
                    <label>31155</label>
                    <onclick>Skin.ToggleSetting(MovieDetailsView_HidePanel)</onclick>
                    <selected>Skin.HasSetting(MovieDetailsView_HidePanel)</selected>
					<visible>Control.IsVisible(53) | Control.IsVisible(54) | Control.IsVisible(505)</visible>
                </control>
				<control type="radiobutton" id="323">
                    <!--Toggle watchedOverlay-->
                    <include>SubMenuButton</include>
                    <label>31094</label>
                    <onclick>Skin.ToggleSetting(HideWatchedOverlayBigPanel)</onclick>
                    <selected>Skin.HasSetting(HideWatchedOverlayBigPanel)</selected>
					<visible>Control.IsVisible(57) | Control.IsVisible(59)</visible>
                </control>
            </control>
        </control>
        <!--Youtube Search Overlay
		<include>YoutubeSearchOverlay</include>	-->
    </controls>
</window>