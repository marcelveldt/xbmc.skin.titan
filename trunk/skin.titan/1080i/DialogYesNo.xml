<?xml version="1.0" encoding="utf-8"?>
<!--Titan-->
<window>
    <defaultcontrol always="true">10</defaultcontrol>
    <zorder>5</zorder>
    <controls>
        <!--Dialog Overlay-->
        <include>GlobalBackground</include>
		<include>DialogOverlay</include>
        <!--Dialog Yes/No-->
        <control type="group">
            <include>animation_dialog</include>
            <control type="group">
                <posx>590</posx>
                <posy>460</posy>
                <control type="image">
                    <posx>-650</posx>
                    <posy>-220</posy>
                    <width>2050</width>
                    <height>480</height>
                    <texture border="39">views/filemanager_panel1.png</texture>
                </control>
                <control type="button" id="11">
                    <description>Yes button</description>
                    <posx>71</posx>
                    <posy>65</posy>
                    <width>300</width>
                    <height>90</height>
                    <onleft>10</onleft>
                    <onright>10</onright>
					<label>107</label>
                    <align>center</align>
                    <font>Light34</font>
                    <texturefocus border="20">dialogs/keyboard/donefocus.png</texturefocus>
                    <texturenofocus border="20">dialogs/keyboard/key-nf.png</texturenofocus>
                    <textcolor>midgrey</textcolor>
                    <focusedcolor>eewhite</focusedcolor>
                    <selectedcolor>eewhite</selectedcolor>
                </control>
                <control type="button" id="10">
                    <description>No button</description>
                    <posx>368</posx>
                    <posy>65</posy>
                    <width>300</width>
                    <height>90</height>
                    <onleft>11</onleft>
                    <onright>11</onright>
                    <align>center</align>
                    <font>Light34</font>
                    <label>106</label>
                    <texturefocus border="20">dialogs/keyboard/cancelfocus.png</texturefocus>
                    <texturenofocus border="20">dialogs/keyboard/key-nf.png</texturenofocus>
                    <textcolor>midgrey</textcolor>
                    <focusedcolor>eewhite</focusedcolor>
                    <selectedcolor>eewhite</selectedcolor>
                </control>
            </control>
            <control type="label" id="1">
                <description>Dialog Header</description>
                <posx>100</posx>
                <posy>280</posy>
                <width>1720</width>
                <align>center</align>
                <aligny>top</aligny>
                <include>DialogHeader_Alt</include>
                <animation effect="slide" end="0,60" time="0" condition="IsEmpty(Control.GetLabel(2))">Conditional</animation>
                <animation effect="slide" end="0,60" time="0" condition="IsEmpty(Control.GetLabel(3))">Conditional</animation>
                <animation effect="slide" end="0,-60" time="0" condition="Window.IsActive(FileManager.xml)">Conditional</animation>
                <!--Doubles Line-->
            </control>
            <control type="label" id="2">
                <description>Line 1</description>
                <posx>100</posx>
                <posy>340</posy>
                <width>1720</width>
                <align>center</align>
                <aligny>top</aligny>
                <label>-</label>
                <include>DialogHeader2</include>
                <animation effect="slide" end="0,60" time="0" condition="IsEmpty(Control.GetLabel(3))">Conditional</animation>
                <animation effect="slide" end="0,-60" time="0" condition="Window.IsActive(FileManager.xml)">Conditional</animation>
                <!--Doubles Line-->
            </control>
            <control type="label" id="3">
                <description>Line 2</description>
                <posx>100</posx>
                <posy>400</posy>
                <width>1720</width>
                <align>center</align>
                <aligny>top</aligny>
                <label>-</label>
                <include>DialogHeader</include>
            </control>
            <!--control type="label" id="4">
				<description>Line 3</description>
				<posx>100</posx>
				<posy>400</posy>
				<width>1720</width>				
				<align>center</align>
				<aligny>top</aligny>
				<label>-</label>
				<include>DialogHeader2</include>				
			</control-->
        </control>
    </controls>
</window>